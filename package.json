{
  "name": "vue-library",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/my-component-library.umd.js",
  "module": "dist/my-component-library.es.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build:only": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.18"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^5.0.0",
    "tinymce": "^6.3.1",
    "vue-router": "^4.1.6"
  }
}
